XTerm*termName:xterm-256color
XTerm*allowWindowOps:on
XTerm*reverseVideo:off
XTerm*fullscreen:never
XTerm*faceName:Droid Sans Mono Slashed for Powerline
XTerm*faceSize:8
XTerm*VT100.geometry: 80x24
XTerm*font: 7x13
XTerm*metaSendsEscape:true

Xcursor*size: 16

URxvt.perl-ext-common: osc52
# URxvt.keysym.S-Insert: eval:paste_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt*reverseVideo:off
URxvt*font:xft:Droid Sans Mono Slashed for Powerline:style=Regular:size=8:minspace=False
URxvt*boldFont:xft:Droid Sans Mono Slashed for Powerline:style=Regular:size=8:minspace=False
URxvt*italicFont:xft:Droid Sans Mono Slashed for Powerline:style=Regular:size=8:minspace=False
URxvt*boldItalicFont:xft:Droid Sans Mono Slashed for Powerline:style=Regular:size=8:minspace=False
URxvt*letterSpace:-1
URxvt*lineSpace:0
URxvt*scrollBar:false

URxvt*foreground: rgb:c0/c0/c0
URxvt*background: rgb:00/00/00
